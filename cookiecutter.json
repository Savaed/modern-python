{
    "project_name": "sample project",
    "project_slug": "{{ cookiecutter.project_name | lower | replace('-', '_') | replace(' ', '_') | replace('.', '_') | trim }}",
    "friendly_name": "{{ cookiecutter.project_slug | replace('_', ' ') | title | trim }}",
    "author": "Jon Doe",
    "github_user": "github_user",
    "mail": "sample@example.com",
    "project_description": "{{ cookiecutter.friendly_name }} description",
    "version": "0.1.0",
    "python_version": "3.11",
    "poetry_version": "1.6.1",
    "tox_version": "4.11.3",
    "git_main_branch": "main",
    "use_vscode": true,
    "use_hypothesis": true,
    "use_docker": false,
    "use_pre_commit_ci": false,
    "use_github_actions": true,
    "github_ci_os": "ubuntu-latest",
    "codecov_token_name": "CODECOV_TOKEN",
    "add_citation": false,
    "line_length": 100,
    "license": [
        "MIT",
        "BSD",
        "ISC",
        "Apache Software License 2.0",
        "GNU General Public License v3",
        "none"
    ],
    "__python_version_str": "{{ cookiecutter.python_version.split('.')[:2] | join | lower }}",
    "__current_year": "{% now 'utc', '%Y' %}",
    "__prompts__": {
        "use_pre_commit_ci": "Do you want to use pre-commit CI?",
        "use_docker": "Do you want to use Docker?",
        "project_name": "Project name",
        "project_slug": "Python package name",
        "friendly_name": "Friendly project name (e.g. for README)",
        "project_description": "Project description",
        "version": "Project version",
        "python_version": "Python version",
        "poetry_version": "Poetry version",
        "tox_version": "tox version",
        "author": "Author full name",
        "mail": "Author e-mail addres",
        "codecov_token_name": "Codecov Github token name",
        "git_main_branch": "The main git branch",
        "use_hypothesis": "Do you want to use hypothesis for testing?",
        "use_vscode": "Do you want to use vscode as IDE?",
        "use_github_actions": "Do you want to use Github Actions for CI/CD?",
        "github_ci_os": "Github Actions OS",
        "github_user": "Github username",
        "add_citation": "Do you want to add software/paper citation?",
        "line_length": "Max. line length",
        "license": "Which license type do you want to use?"
    }
}
